   "source": [
    "item_name = []\n",
    "prices = []\n",
    "item_link=[] \n",
    "description = []\n",
    "origin = []\n",
    "shipping = []\n",
    "for i in range(1,44):\n",
    " \n",
    "    ebayUrl = \"https://www.ebay.com/sch/i.html?_from=R40&_nkw=robotic+vacuum+cleaner&_sacat=0&_pgn=\" + str(i)\n",
    "    r= requests.get(ebayUrl, headers=headers)\n",
    "    soup=bs(r.content, \"lxml\")\n",
    "    listings = soup.find_all('li', attrs={'class': 's-item'})\n",
    "   \n",
    " \n",
    "    for listing in listings:\n",
    "        prod_link= \" \"\n",
    "        prod_name=\" \"\n",
    "        prod_price = \" \"\n",
    "        item_desc = \" \"\n",
    "        item_loc = \" \"\n",
    "        item_shipping=\" \"\n",
    "        for name in listing.find_all('h3', attrs={'class':\"s-item__title\"}):\n",
    "            if(str(name.find(text=True, recursive=False))!=\"None\"):\n",
    "                prod_name=str(name.find(text=True, recursive=False))\n",
    "                item_name.append(prod_name)\n",
    "             \n",
    "        if(prod_name!=\" \"):\n",
    "            price = listing.find('span', attrs={'class':\"s-item__price\"})\n",
    "            prod_price = str(price.find(text=True, recursive=False))\n",
    "            prices.append(prod_price)\n",
    "            \n",
    "        if(prod_name!=\" \"):\n",
    "            prod_link=listing.find('a', attrs={'class':\"s-item__link\"}).get('href')\n",
    "            item_link.append(prod_link)\n",
    "        \n",
    "        if(prod_name!=\" \"):\n",
    "            desc = listing.find('div', class_='s-item__subtitle')\n",
    "            item_desc = str(desc.find(text=True, recursive=False))\n",
    "            description.append(item_desc)\n",
    "        \n",
    "        if(prod_name!=\" \"):\n",
    "            item_origin = listing.find('span', class_=\"s-item__location s-item__itemLocation\")\n",
    "            item_loc = str(item_origin.find(text=True, recursive=False))\n",
    "            origin.append(item_loc)\n",
    "         \n",
    "        if(prod_name!= \" \"):\n",
    "            item_shipping = listing.find('span', class_='s-item__shipping s-item__logisticsCost')\n",
    "            shipping.append(item_shipping)\n",
    "    \n",
    "    \n",
    "ebay ={\n",
    "        \"Name\":item_name, \n",
    "        \"Prices\": prices, \n",
    "        \"Description\": description,\n",
    "        \"Origin\": origin,\n",
    "        \"Shipping\":shipping,\n",
    "        \"Link\":item_link, \n",
    "    \n",
    "       }\n",
    "\n",
    "df = pd.DataFrame.from_dict(ebay)\n",
    
   ]
